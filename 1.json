{
    "title": "Launch apps",
    "rules": [
        {
            "description": "「right-shift + S」でSlackを開く",
            "manipulators": [
                {
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "right_shift"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Slack.app'"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
